{
  "name": "delivery-management-frontend",
  "version": "1.0.0",
  "description": "Google Sheets-based delivery management system frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmitOnError false && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.3.0",
    "vue-router": "^4.2.0",
    "pinia": "^2.1.0",
    "axios": "^1.4.0",
    "vuetify": "^3.3.0",
    "@mdi/font": "^7.2.96",
    "qrcode-reader": "^1.0.4",
    "html5-qrcode": "^2.3.8"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.0",
    "vite": "^4.4.0",
    "vite-plugin-pwa": "^0.16.4",
    "vitest": "^0.33.0",
    "vue-tsc": "^2.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
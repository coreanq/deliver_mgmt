{
  "cli": {
    "version": ">= 13.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "channel": "preview",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "EAS_BUILD_DISABLE_MAVEN_CACHE": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6757303714"
      }
    }
  }
}
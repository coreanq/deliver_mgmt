{
  "name": "deliver-mgmt-worker",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "dev:web": "cd web && npm run dev",
    "build:web": "cd web && npm install && npm run build",
    "deploy": "wrangler deploy",
    "deploy:full": "npm run build:web && wrangler deploy",
    "db:migrate": "wrangler d1 execute deliver-mgmt-db --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute deliver-mgmt-db --file=./schema.sql --local",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "hono": "^4.6.0",
    "jose": "^5.9.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.6.0",
    "wrangler": "^4.0.0"
  }
}

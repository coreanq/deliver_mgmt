# 서버 설정
PORT=5000
HOST=0.0.0.0
NODE_ENV=development

# 프론트엔드 설정
FRONTEND_URLS=http://localhost:3000,http://localhost:3001,http://localhost:3002

# 세션 설정
SESSION_SECRET=your-session-secret-key

# JWT 설정
JWT_SECRET=your-jwt-secret-key

# Google OAuth2 설정
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:5000/api/auth/google/callback

# SOLAPI OAuth2 설정
SOLAPI_CLIENT_ID=your-solapi-client-id
SOLAPI_CLIENT_SECRET=your-solapi-client-secret
SOLAPI_REDIRECT_URI=http://localhost:5000/api/solapi/auth/callback

# QR 코드 보안키
QR_SECRET_KEY=your-qr-secret-key

# 로그 설정
LOG_LEVEL=info
LOG_FILE=logs/app.log